{
  "manifest_version": 2,
  "name": "PDFSketch",
  "version": "0.0.7",
  "minimum_chrome_version": "33",
  "icons": {
    "16": "logo16.png",
    "128": "logo128.png"
  },
  "permissions": [
    {"fileSystem": ["write"]},
    "clipboardRead",
    "clipboardWrite"
  ],
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  }
}
